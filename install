echo 'source ~/GitHub/rkiel/git-utilities/dotfiles/git-prompt.sh' >> ~/.bash_profile

echo "export FEATURE_USER=$1" >> ~/.bash_profile

echo 'source ~/GitHub/rkiel/git-utilities/dotfiles/bashrc' >> .bashrc

